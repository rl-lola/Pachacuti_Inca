<!--#Terrace Farm Improvement-->

		<Row Tag="LOC_IMPROVEMENT_RL_TERRACE_FARM_NAME" Language="en_US">
			<Text>Terrace Farm</Text>
		</Row>

		<Row Tag="LOC_IMPROVEMENT_RL_TERRACE_FARM_DESCRIPTION" Language="en_US">
